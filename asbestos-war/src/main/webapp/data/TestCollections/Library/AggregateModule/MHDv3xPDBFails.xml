<TestScript xmlns="http://hl7.org/fhir">
    <modifierExtension url="urn:module">
        <extension url="urn:variable-in">
            <valueString value="isComprehensiveMetadataExpected"/>
        </extension>
        <extension url="urn:variable-in">
            <valueString value="getFixtureParams"/>
        </extension>
        <extension url="urn:variable-in">
            <valueString value="expectedFixtureBundleEntryCount"/>
        </extension>
        <extension url="urn:variable-in">
            <valueString value="expectedManifestCount"/>
        </extension>
        <extension url="urn:variable-in">
            <valueString value="expectedDocRefCount"/>
        </extension>
    </modifierExtension>

    <id value="MHDv3xPDBFailsAggregate"/>
    <name value="MHDv3xPDBFailsAggregate"/>
    <description value="This TestScript is a bridge that links the parent TestScript to the imported module component."/>

    <test>
        <description value="Bundle evaluation assertions."/>
        <action>
            <operation>
                <description value="Previous test action operation is analyzed using assertions."/>
                <modifierExtension url="https://github.com/usnistgov/asbestos/wiki/TestScript-Import">
                    <extension url="component">
                        <valueString value="../MHDv3x/PDBFailsMain.xml"/>
                    </extension>
                    <extension url="urn:variable-in">
                        <valueString value="isComprehensiveMetadataExpected"/>
                    </extension>
                    <extension url="urn:variable-in">
                        <valueString value="getFixtureParams"/>
                    </extension>
                    <extension url="urn:variable-in">
                        <valueString value="expectedFixtureBundleEntryCount"/>
                    </extension>
                    <extension url="urn:variable-in">
                        <valueString value="expectedManifestCount"/>
                    </extension>
                    <extension url="urn:variable-in">
                        <valueString value="expectedDocRefCount"/>
                    </extension>
                </modifierExtension>
            </operation>
        </action>
    </test>
</TestScript>
