<TestScript xmlns="http://hl7.org/fhir">

    <variable>
        <name value="expectedFailAssertionIdList"/>
        <defaultValue value="('TR_1')"/>
    </variable>

    <test>
        <description value="Submit Provide Document Bundle to MHD Document Recipient. If this transaction targets a DocumentRecipient coupled to an XDS DocumetDocumentReference contains a fixed id so the second time (or after) you submit it will fail with a message: Submission includes pre-assigned id which already exists. This is ok."/>
        <action>
            <operation>
                <description value="Module usage description"/>
                <modifierExtension url="https://github.com/usnistgov/asbestos/wiki/TestScript-Import">
                    <extension url="component">
                        <valueString value="../../Library/AggregateModule/MHDPDBFails.xml"/>
                    </extension>
                    <extension url="urn:variable-in">
                        <valueString value="'true'"/>
                    </extension>
                    <extension url="urn:variable-in">
                        <valueString value="'?fixtureId=pdb'"/>
                    </extension>
                    <extension url="urn:variable-in">
                        <valueString value="'3'"/>
                    </extension>
                    <extension url="urn:variable-in">
                        <valueString value="'0'"/>
                    </extension>
                    <extension url="urn:variable-in">
                        <valueString value="'0'"/>
                    </extension>
                    <extension url="urn:variable-in">
                        <valueString value="expectedFailAssertionIdList"/>
                    </extension>
                </modifierExtension>
            </operation>
        </action>
    </test>
</TestScript>
