<TestScript xmlns="http://hl7.org/fhir">
    <extension url="urn:component-parameters">
        <extension url="urn:parameter-in">
            <valueString  value="pdbRequest"/>
        </extension>
        <extension url="urn:parameter-out">
            <valueString value="pdbResponse"/>
        </extension>
    </extension>

    <url value="xxx"/>
    <name value="xxx"/>
