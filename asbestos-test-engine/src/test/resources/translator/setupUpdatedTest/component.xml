    <setup>
        <action>
            <operation>
                <type>
                    <system value="https://github.com/usnistgov/asbestos/wiki/Testscript-Operation-Codes"/>
                    <code value="mhd-pdb-transaction"/>
                </type>
                <responseId value="pdbResponse"/>
                <sourceId value="pdbRequest"/>
            </operation>
        </action>
    </setup>
</TestScript>
